<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UserController</servlet-name>
  	<display-name>UserControllor</display-name>
  	<description></description>
  	<servlet-class>com.kami.app.key.controller.UserController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/user</url-pattern>
  </servlet-mapping>
    <servlet>
        <servlet-name>KeyController</servlet-name>
        <display-name>KeyController</display-name>
        <description></description>
        <servlet-class>com.kami.app.key.controller.KeyController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>KeyController</servlet-name>
        <url-pattern>/*.execute</url-pattern>
    </servlet-mapping>

</web-app>
